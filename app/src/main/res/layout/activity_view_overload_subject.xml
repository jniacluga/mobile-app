<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="pup.com.gsouapp.Activities.ViewServiceApplicationActivity">

    <include layout="@layout/general_service_application_details" />

    <TextView
        android:id="@+id/view_os_student_status"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Student Status" />

    <TextView
        android:id="@+id/view_os_number_of_subjects"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Number of Subjects" />

    <TextView
        android:id="@+id/view_os_list_of_subjects"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="List of Subjects" />

    <TextView
        android:id="@+id/view_os_reason"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Reason" />

</LinearLayout>
