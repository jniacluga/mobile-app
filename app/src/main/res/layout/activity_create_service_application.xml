<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="pup.com.gsouapp.Activities.CreateServiceApplicationActivity">

    <Spinner
        android:id="@+id/service_application_option"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:entries="@array/service_application_filters"/>

    <FrameLayout
        android:id="@+id/service_application_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1">

    </FrameLayout>

</LinearLayout>
